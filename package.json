{
    "engines": {
        "node": "14.4.0"
    },
    "devDependencies": {
        "@types/jest": "^26.0.0",
        "@types/react": "^16.9.36",
        "@types/react-dom": "^16.9.8",
        "@types/react-helmet": "^6.0.0",
        "@types/theme-ui": "^0.3.3",
        "@types/uuid": "^8.0.0",
        "@typescript-eslint/eslint-plugin": "^3.2.0",
        "@typescript-eslint/parser": "^3.2.0",
        "eslint": "^7.2.0",
        "eslint-config-google": "^0.14.0",
        "eslint-plugin-node": "^11.1.0",
