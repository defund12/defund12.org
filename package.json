        "gatsby-plugin-sitemap": "^2.4.5",
        "gatsby-source-filesystem": "^2.3.11",
        "gatsby-transformer-remark": "^2.8.15",
        "gatsby-transformer-yaml": "^2.4.4",
        "jest": "25.5.4",
        "node-sass": "^4.14.1",
        "query-string": "^6.13.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-helmet": "^6.1.0",
        "react-select": "^3.1.0",
        "rehype-sanitize": "^3.0.1",
        "rehype-stringify": "8.0.0",
        "remark-breaks": "^1.0.5",
        "remark-parse": "^8.0.2",
        "remark-rehype": "^7.0.0",
        "tslint": "^6.1.2",
        "typescript": "^3.9.5",
        "unified": "^9.0.0",
        "uuid": "^8.1.0",
        "webpack": "^4.43.0",
        "fs": "0.0.1-security",
        "handlebars": "^4.7.6",
        "puppeteer": "^3.2.0"
